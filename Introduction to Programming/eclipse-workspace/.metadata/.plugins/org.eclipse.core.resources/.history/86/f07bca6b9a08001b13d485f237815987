class Fucntions {
	
	static int square(int x){
		return x*x;
	}
	
	static int dobro(int x){
		return x+x;
	}
	
	static int diferenca(int x, int y){
		return x-y;
	}
	
	static double percentage (int n, int total){
		return n/total;
	}
	
	static double media(int a, int b){
		return (a+b)/2;
	}
	
	static int round(double a){
		return (int)Math.round(a);
	}
	
	static boolean isNeg(int a){
		return a<0;
	}
	
	static boolean isOdd(int a){
		return a%2==1;
	}
	
	static boolean isEven(int a){
		return a%2==0;
	}
	
	
	static boolean isMultiple(int a, int b){
		return a%b==0 || b%a==0;
	}
	
	static boolean isSingle(int a){
		return a>-1 && a<10;
	}
	
	static boolean isIncluded(int a, int b, int c){
		return a>=b && a<=c;
	
	static boolean isExcluded(int a, int b, int c){
		return !isIncluded(a,b,c);
	}
	
	static boolean xor(boolean a, boolean b){
		if(a || b){
			return true;
		return false;
	
		

}